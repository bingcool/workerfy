{
  "name": "bingcool/workerfy",
  "description": "workerfy is a cli Framework based on swoole extension, for everyone use it easily!",
  "keywords": [
    "workerfy",
    "swoole",
    "php",
    "cli",
    "console"
  ],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "bingcool",
      "email": "2437667702@qq.com",
      "role": "cli multi worker process model of php"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php" : ">=7.1.0",
    "psr/log" : "*",
    "ext-swoole" : ">=4.4.5",
    "ext-pcntl" : "*",
    "ext-posix" : "*",
    "ext-json": "*",
    "ext-sysvmsg": "*",
    "ext-sockets": "*",
    "ext-redis": "*",
    "ext-pdo": "*",
    "ext-curl": "*",
    "dragonmantank/cron-expression" : "2.3.0",
    "bingcool/library": "dev-master",
    "malkusch/lock": "dev-master",
    "php-amqplib/php-amqplib": "^3.0@dev",
    "php-ffmpeg/php-ffmpeg": "v1.x-dev",
    "symfony/string": "5.4.x-dev",
    "slim/slim": "4.*",
    "slim/psr7": "^1.4",
    "nyholm/psr7": "1.4.x-dev",
    "nyholm/psr7-server": "dev-master",
    "guzzlehttp/psr7": "2.1.x-dev",
    "http-interop/http-factory-guzzle": "^1.1",
    "laminas/laminas-diactoros": "2.4.x-dev"
  },
  "require-dev": {
    "kwn/php-rdkafka-stubs": "@dev",
    "swoole/ide-helper": "@dev"
  },
  "autoload": {
    "files": ["src/Ctrl.php","src/Function.php"],
    "psr-4": {
      "Workerfy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Workerfy\\Tests\\": "tests/"
    }
  }

}
